# Ed-Fi API Configuration
# Copy this file to .env and fill in your actual values

# Ed-Fi API Base URL (Discovery API endpoint)
EDFI_BASE_URL=https://api.example.org/v7.3/api

# OAuth 2.0 Client Credentials
EDFI_CLIENT_ID=your_client_id_here
EDFI_CLIENT_SECRET=your_client_secret_here

# School ID for student associations (required for studentSchoolAssociations)
EDFI_SCHOOL_ID=255901001

# Optional: API timeout in seconds
EDFI_TIMEOUT=30